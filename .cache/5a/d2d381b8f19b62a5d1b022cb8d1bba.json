ne":146,"column":40},"generated":{"line":1,"column":1714}},{"source":"../src/Controller/FactoryUser.js","name":"data","original":{"line":146,"column":47},"generated":{"line":1,"column":1716}},{"source":"../src/Controller/FactoryUser.js","name":"user","original":{"line":147,"column":24},"generated":{"line":1,"column":1722}},{"source":"../src/Controller/FactoryUser.js","name":"score","original":{"line":147,"column":29},"generated":{"line":1,"column":1724}},{"source":"../src/Controller/FactoryUser.js","name":"user","original":{"line":148,"column":24},"generated":{"line":1,"column":1732}},{"source":"../src/Controller/FactoryUser.js","name":"score","original":{"line":148,"column":29},"generated":{"line":1,"column":1734}},{"source":"../src/Controller/FactoryUser.js","name":"user","original":{"line":148,"column":37},"generated":{"line":1,"column":1740}},{"source":"../src/Controller/FactoryUser.js","name":"todayScore","original":{"line":148,"column":42},"generated":{"line":1,"column":1742}},{"source":"../src/Controller/FactoryUser.js","name":"user","original":{"line":148,"column":55},"generated":{"line":1,"column":1753}},{"source":"../src/Controller/FactoryUser.js","name":"todayScore","original":{"line":148,"column":60},"generated":{"line":1,"column":1755}},{"source":"../src/Controller/FactoryUser.js","original":{"line":148,"column":71},"generated":{"line":1,"column":1766}},{"source":"../src/Controller/FactoryUser.js","name":"setTimeout","original":{"line":150,"column":20},"generated":{"line":1,"column":1769}},{"source":"../src/Controller/FactoryUser.js","original":{"line":150,"column":31},"generated":{"line":1,"column":1780}},{"source":"../src/Controller/FactoryUser.js","name":"resole","original":{"line":151,"column":24},"generated":{"line":1,"column":1791}},{"source":"../src/Controller/FactoryUser.js","name":"user","original":{"line":151,"column":31},"generated":{"line":1,"column":1793}},{"source":"../src/Controller/FactoryUser.js","original":{"line":152,"column":23},"generated":{"line":1,"column":1797}},{"source":"../src/Controller/FactoryUser.js","name":"catch","original":{"line":154,"column":17},"generated":{"line":1,"column":1804}},{"source":"../src/Controller/FactoryUser.js","original":{"line":154,"column":23},"generated":{"line":1,"column":1810}},{"source":"../src/Controller/FactoryUser.js","name":"error","original":{"line":154,"column":23},"generated":{"line":1,"column":1819}},{"source":"../src/Controller/FactoryUser.js","name":"console","original":{"line":155,"column":20},"generated":{"line":1,"column":1822}},{"source":"../src/Controller/FactoryUser.js","name":"log","original":{"line":155,"column":28},"generated":{"line":1,"column":1830}},{"source":"../src/Controller/FactoryUser.js","original":{"line":155,"column":32},"generated":{"line":1,"column":1834}},{"source":"../src/Controller/FactoryUser.js","name":"error","original":{"line":155,"column":41},"generated":{"line":1,"column":1842}},{"source":"../src/Controller/FactoryUser.js","name":"reject","original":{"line":156,"column":20},"generated":{"line":1,"column":1845}},{"source":"../src/Controller/FactoryUser.js","name":"error","original":{"line":156,"column":27},"generated":{"line":1,"column":1847}}],"sources":{"../src/Controller/FactoryUser.js":"import { url, requestOptions} from './dataApp'\n/**\n * Factory for create a new User a save all user data\n * You can do this for create a factory :\n * const factUser = new FactoryUser();\n * factUser.CreateElement(userID);\n * @see {@link CreateElement} for construct an user\n * @function FactoryUser\n * @method CreateElement\n * @returns {object} A factory for construct an user\n */\nexport default function FactoryUser(){\n    /**\n     * Function for create a new instance\n     * @method CreateElement\n     * @see {@link FactoryUser} for create instance of this factory\n     * @param {number} id id of the user\n     * @returns user\n     */\n\n    this.CreateElement = function (id) {\n        const user = {id}\n        /**\n         * @param {string} slug endpoint of the API\n         * @param {func} modififyResponse function for return a good data schema\n         * @param {string} attrName the name of where data will save in user object\n         * @Promise user\n         */\n        user.loadData = (slug, modififyResponse, attrName) => new Promise((resole, reject) => {\n            fetch(`${url}/user/${user.id}/${slug}`, requestOptions)\n                .then(response => response.json())\n                .then(result => {\n                    const newAttr = attrName ? attrName : slug.replaceAll('-', '')\n                    user[newAttr] = user[modififyResponse](result.data)\n                    setTimeout(() => {\n                        resole(user)\n                    }, 2000);\n                })\n                .catch(error => {\n                    console.log('error', error)\n                    reject(error)\n                });\n        })\n\n        /**\n         * Promise who return the data for graph Timing\n         * @Promise user\n         */\n        user.loadTiming = () => new Promise((resole, reject) => {\n            user.loadData('average-sessions', 'normalChange','timing')\n            .then(user=>resole(user))\n        })\n\n        /**\n         * Promise who return the data for graph Activity\n         * @Promise user\n         */\n        user.loadActivity = () => new Promise((resole, reject) => {\n            user.loadData('activity', 'normalChange')\n            .then(user=>resole(user))\n        })\n\n        /**\n         * Promise who return the data for graph Intensity\n         * @Promise user\n         */\n        user.loadIntensity = () => new Promise((resole, reject) => {\n            user.loadData('performance',  'intensityChange','intensity')\n            .then(user=>resole(user))\n        })\n\n        /**\n         * return normal data\n         * @return data\n         */\n        user.normalChange=(data)=>{\n            return data.sessions\n        }\n\n        /**\n         * return intensity data\n         * @return data\n         */\n        user.intensityChange = (data) => {\n            const order = ['intensity', 'speed', 'strength', 'endurance', 'energy','cardio']\n            const translate = ['Intensité', 'Vitesse', 'Force', 'Endurance', 'Force','Cardio']\n            const tempData = data.data.map(x => {\n                x.label = data.kind[x.kind]\n                return x\n            })\n            const newData=[]\n            let i = 0\n            order.forEach(x=>{\n                const attr = tempData.find(elt => elt.label===x)\n                attr.labelfr = translate[i]\n                newData.push(attr)\n                i = i + 1\n            })\n            return newData\n        }\n\n        /**\n         * return timing data\n         * @return data\n         */\n        user.timingChange=(data)=>{\n            const newData = data.sessions.map(x=>{\n                switch (x.day) {\n                    case 1:\n                        x.label = 'L'\n                        break;\n                    case 2:\n                        x.label='M'\n                        break;\n                    case 3:\n                        x.label='M'\n                        break;\n                    case 4:\n                        x.label='J'\n                        break;\n                    case 5:\n                        x.label='V'\n                        break;\n                    case 6:\n                        x.label='S'\n                        break;\n                    case 7:\n                        x.label='D'\n                        break;\n\n                    default:\n                        x.label=''\n                        break;\n                }\n\n                return x\n\n            })\n            return newData\n        }\n\n        return new Promise((resole, reject) => {\n            fetch(`${url}/user/${user.id}`, requestOptions)\n                .then(response => response.json())\n                .then(result => {\n                    Object.assign(user, result.data)\n                    if(!user.score){\n                        user.score = user.todayScore ? user.todayScore:0\n                    }\n                    setTimeout(() => {\n                        resole(user)\n                    }, 2000);\n                })\n                .catch(error => {\n                    console.log('error', error)\n                    reject(error)\n                });\n        })\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"f4b3c0173b6c3d194c92a8ce4e53d39f","cacheData":{"env":{}}}