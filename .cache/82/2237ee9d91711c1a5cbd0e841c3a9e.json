"column":6},"generated":{"line":1,"column":1858}},{"source":"../src/Components/Score.js","original":{"line":71,"column":16},"generated":{"line":1,"column":1868}},{"source":"../src/Components/Score.js","name":"score","original":{"line":77,"column":4},"generated":{"line":1,"column":1869}},{"source":"../src/Components/Score.js","name":"PropTypes","original":{"line":77,"column":10},"generated":{"line":1,"column":1875}},{"source":"../src/Components/Score.js","name":"number","original":{"line":77,"column":20},"generated":{"line":1,"column":1877}},{"source":"../src/Components/Score.js","name":"number","original":{"line":77,"column":20},"generated":{"line":1,"column":1885}},{"source":"../src/Components/Score.js","name":"isRequired","original":{"line":77,"column":27},"generated":{"line":1,"column":1892}}],"sources":{"../src/Components/Score.js":"import PropTypes from 'prop-types';\nimport { useEffect, useRef, useState } from \"react\";\nimport { RadialBarChart, RadialBar, Legend } from \"recharts\";\nimport { RenderLegendScore, style} from \"./GraphComponent/score\"\n\n/**\n * Use Activity component for create the graph activity\n * @module Score\n * @component\n * @category Recharts\n * @param {object} param\n * @param {number} props.score is a number > 1\n * @example\n * const score = .3;\n * return ( <Score score={score}/> )\n */\nexport default function Score({score}) {\n    const [width, setWidth] = useState(null)\n    const graphRef = useRef(null);\n\n    /**\n     * Set the with of this component\n     */\n    useEffect(() => {\n        const { offsetWidth } = graphRef.current\n        if (width !== offsetWidth) {\n            setWidth(offsetWidth)\n        }\n    }, [width])\n\n    /**\n     * the data of this component\n     */\n    const data = [{\n            value: score,\n            fill: \"#FF0000\"\n    }];\n\n    return (\n        <div className=\"score\"\n            ref={graphRef}\n            >\n                <RadialBarChart\n                    width={width}\n                    height={width}\n                    innerRadius={width/2}\n                    barSize={10}\n                    data={data}\n                    startAngle={90}\n                    endAngle={(data[0].value * 360 + 90)}\n                >\n                    <RadialBar\n                        minAngle={15}\n                        cornerRadius={50}\n                        dataKey=\"value\"\n                    />\n                    <Legend\n                        iconSize={10}\n                        width={120}\n                        height={140}\n                        layout=\"vertical\"\n                        verticalAlign=\"middle\"\n                        content={<RenderLegendScore/>}\n                        wrapperStyle={style.box}\n                    />\n                </RadialBarChart>\n        </div>\n    );\n}\n\nScore.prototype={\n    /**\n     * Score is a number > 1 (percentage : 1 === 100%)\n     *\n     * @var {number}\n     */\n    score:PropTypes.number.isRequired\n}\n"},"lineCount":null}},"error":null,"hash":"102c73e794e63d2919bdd3f47a36a561","cacheData":{"env":{}}}