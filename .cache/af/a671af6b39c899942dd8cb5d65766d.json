y\"\n        >\n            {!data ? <Spinner /> :\n                <ResponsiveContainer aspect={998/382}>\n                    <BarChart\n                        data={data}\n                        margin={{\n                            top: 5,\n                            right: 30,\n                            left: 20,\n                            bottom: 5\n                        }}\n                    >\n                        <CartesianGrid\n                            strokeDasharray=\"2 2\"\n                            vertical={false}\n                        />\n                        <XAxis\n                            dataKey=\"name\"\n                            axisLine={true}\n                            tickLine={false}\n                        />\n                        <YAxis\n                            axisLine={false}\n                            tickLine={false}\n                            orientation={'right'}\n                        />\n                        <Tooltip\n                            content={<RenderTooltip />}\n                        />\n                        <Legend\n                            content={<RenderLegend />}\n                            verticalAlign=\"top\"\n                            align=\"right\"\n                            height={80}\n                        />\n                        <Bar\n                            dataKey=\"kilogram\"\n                            fill=\"#282D30\"\n                            shape={<RenderBar />}\n                            barSize={7}\n                            unit={'kg'}\n                        />\n                        <Bar\n                            dataKey=\"calories\"\n                            fill=\"#E60000\"\n                            shape={<RenderBar />}\n                            barSize={7}\n                            unit={'Kcal'}\n                        />\n                    </BarChart>\n                </ResponsiveContainer>\n            }\n        </div>\n    )\n}\n\n\nActivity.propTypes = {\n    /**\n     * Array of sessions objects with properties :<br>\n     * {day: {string}, kilogram : {number}, calories : {number}}\n     */\n    data: PropTypes.arrayOf(PropTypes.shape({\n        day: PropTypes.string.isRequired,\n        kilogram: PropTypes.number.isRequired,\n        calories: PropTypes.number.isRequired,\n    }))\n}\n"},"lineCount":null}},"error":null,"hash":"5e986c8ad794fc2a96d8322afcbfedbb","cacheData":{"env":{}}}